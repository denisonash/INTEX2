                        <li class="nav-item ">
                            <a class="nav-link text-info" asp-controller="Home" asp-action="Story">Predictor</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link text-info" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <partial name="_LoginPartial" />
                    </ul>
                </div>
            </nav>
