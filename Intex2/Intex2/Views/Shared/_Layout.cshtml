    <header class="navigation fixed-top" style="background-color:white">
        <div class="container" >
            <!-- main nav -->
            <nav class="navbar navbar-expand-lg navbar-light" >
                <!-- logo -->
                <a class="navbar-brand logo" asp-controller="Home" asp-action="Index">
                    <i class="fas fa-car-burst">Home</i>
                </a>
                <!-- /logo -->
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation"
                        aria-controls="navigation" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navigation">
                    <ul class="navbar-nav ml-auto text-center">
                        <li class="nav-item dropdown active">
                            <a class="nav-link text-info" asp-controller="Home" asp-action="Index">Homepage</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link text-info" asp-controller="Home" asp-action="ViewCrashes">Crash Data</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link text-info" asp-controller="Home" asp-action="StatsBreakdown">Stats</a>
